{
  "$schema": "http://localhost:8080/jsoncmd-schema.json",
  "name": "my-testing-package",
  "help": "a testing package",
  "commands": [
    {
      "name": "my-echo",
      "syntax": "my-echo <text>",
      "help": "echoes whatever you want!",
      "listeners": [
        {
          "on": "ran",
          "actions": [
            {
              "type": "say",
              "args": ["$$argstr"]
            }
          ]
        }
      ]
    },
    {
      "name": "do",
      "syntax": "do <action>",
      "help": "makes the bot do <action> action",
      "listeners": [
        {
          "on": "ran",
          "actions": [
            {
              "type": "do",
              "args": ["$$argstr"]
            }
          ]
        }
      ]
    }
  ]
}
